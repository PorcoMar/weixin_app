  <view class="floatL">
    <view class="leftm">
      <view class="rmb">¥<text> {{item.value}}</text></view>
      <view class="imgpp">
        <image  src="{{item.logoSmall}}" style="width:180rpx;"></image>
      </view>
    </view>
    <view class="rightm">
      <view class="txt">
        <view class="vue"></view> 
        满{{item.limitFeeMin}}可用 
      </view>
      <view class="txt">
      <view class="vue"></view> 
      <view wx:if="{{item.beginTime}}">{{item.beginTime}}-{{item.endTime}}</view>
      <view wx:else>{{item.validDays}}天内有效</view>
      </view>
      <view class="txt">
      <view class="vue"></view> 
      {{item.name}}
      </view>
    </view>
  </view>
</view>
<view style="height:12rpx;"></view>
